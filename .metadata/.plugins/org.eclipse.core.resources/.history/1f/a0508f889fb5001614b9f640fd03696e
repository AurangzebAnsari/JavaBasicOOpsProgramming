package ClassAndObject;

public class ExampleOfClassAndobject {
	//it is data mamber and also instance variable
			int rollno;
			String name;
			double marks; 
			public static void main(String args[])
			{
				ExampleOfClassAndobject example=new ExampleOfClassAndobject();
				System.out.println(example.rollno);
				System.out.println(example.name);
				System.out.println(example.marks);
			}


}
